<title>Insert title here</title>
</head>
<body ng-controller ="listController">
<h2>List Of Accounts Holders</h2>

<table class="table" border="1">
    <tr>
        
       <th>NAME</th>
       <th>EMAIL</th> 
        <th>PASSWORD</th>        
    </tr>
    <tr ng-repeat = "accList in listAccount | filter:searchText">         
      <td>{{accList.name}}</td>
      <td>{{accList.email}}</td>
      <td>{{accList.password}}</td>
      
    </tr>
</table>

</body>

</html>